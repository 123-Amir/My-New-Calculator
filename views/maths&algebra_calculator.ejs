<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Math and Algebra Calculators</title>
    <link rel="stylesheet" href="maths&algebra_calculator.css">
</head>
<body>
    <div class="container">
        <h1>Math and Algebra Calculators</h1>

        <!-- Complex Number Calculator -->
        <div class="calculator">
            <h2>Complex Number Calculator</h2>
            <label for="real1">Real Part (Complex 1):</label>
            <input type="number" id="real1" placeholder="Enter Real Part 1">
            <label for="imag1">Imaginary Part (Complex 1):</label>
            <input type="number" id="imag1" placeholder="Enter Imaginary Part 1">
            <label for="real2">Real Part (Complex 2):</label>
            <input type="number" id="real2" placeholder="Enter Real Part 2">
            <label for="imag2">Imaginary Part (Complex 2):</label>
            <input type="number" id="imag2" placeholder="Enter Imaginary Part 2">
            <button onclick="calculateComplexNumber()">Calculate</button>
            <div class="result" id="complex-result"></div>
        </div>

        <!-- Boolean Algebra Simplifier -->
        <div class="calculator">
            <h2>Boolean Algebra Simplifier</h2>
            <label for="boolean-expression">Boolean Expression:</label>
            <input type="text" id="boolean-expression" placeholder="Enter Boolean Expression">
            <button onclick="simplifyBooleanAlgebra()">Simplify</button>
            <div class="result" id="boolean-result"></div>
        </div>

        <!-- Partial Fraction Decomposer -->
        <div class="calculator">
            <h2>Partial Fraction Decomposer</h2>
            <label for="rational-function">Rational Function:</label>
            <input type="text" id="rational-function" placeholder="Enter Rational Function (e.g. 1/x+2)">
            <button onclick="decomposePartialFractions()">Decompose</button>
            <div class="result" id="partial-fraction-result"></div>
        </div>

        <!-- Inequality Calculator -->
        <div class="calculator">
            <h2>Inequality Calculator</h2>
            <label for="inequality-expression">Inequality Expression:</label>
            <input type="text" id="inequality-expression" placeholder="Enter Inequality Expression">
            <button onclick="calculateInequality()">Solve</button>
            <div class="result" id="inequality-result"></div>
        </div>

        <!-- Flow Rate Calculator -->
        <div class="calculator">
            <h2>Flow Rate Calculator</h2>
            <label for="pipe-diameter">Pipe Diameter (m):</label>
            <input type="number" id="pipe-diameter" placeholder="Enter Pipe Diameter">
            <label for="flow-velocity">Flow Velocity (m/s):</label>
            <input type="number" id="flow-velocity" placeholder="Enter Flow Velocity">
            <button onclick="calculateFlowRate()">Calculate</button>
            <div class="result" id="flow-rate-result"></div>
        </div>

        <!-- Impulse Calculator -->
        <div class="calculator">
            <h2>Impulse Calculator</h2>
            <label for="force">Force (N):</label>
            <input type="number" id="force" placeholder="Enter Force">
            <label for="time">Time (s):</label>
            <input type="number" id="time" placeholder="Enter Time">
            <button onclick="calculateImpulse()">Calculate</button>
            <div class="result" id="impulse-result"></div>
        </div>

        <!-- Factorization Calculator -->
        <div class="calculator">
            <h2>Factorization Calculator</h2>
            <label for="integer">Integer:</label>
            <input type="number" id="integer" placeholder="Enter Integer">
            <button onclick="calculateFactorization()">Factorize</button>
            <div class="result" id="factorization-result"></div>
        </div>

        <!-- Percentage Calculator -->
        <div class="calculator">
            <h2>Percentage Calculator</h2>
            <label for="original-value">Original Value:</label>
            <input type="number" id="original-value" placeholder="Enter Original Value">
            <label for="percentage">Percentage:</label>
            <input type="number" id="percentage" placeholder="Enter Percentage">
            <button onclick="calculatePercentage()">Calculate</button>
            <div class="result" id="percentage-result"></div>
        </div>

        <!-- Exponential Formulas Calculator -->
        <div class="calculator">
            <h2>Exponential Formulas Calculator</h2>
            <label for="base">Base:</label>
            <input type="number" id="base" placeholder="Enter Base">
            <label for="exponent">Exponent:</label>
            <input type="number" id="exponent" placeholder="Enter Exponent">
            <button onclick="calculateExponential()">Calculate</button>
            <div class="result" id="exponential-result"></div>
        </div>

        <!-- Cube of a Number Calculator -->
        <div class="calculator">
            <h2>Cube of a Number Calculator</h2>
            <label for="cube-number">Number:</label>
            <input type="number" id="cube-number" placeholder="Enter Number">
            <button onclick="calculateCube()">Calculate</button>
            <div class="result" id="cube-result"></div>
        </div>

        <!-- Cube Root Calculator -->
        <div class="calculator">
            <h2>Cube Root Calculator</h2>
            <label for="cube-root-number">Number:</label>
            <input type="number" id="cube-root-number" placeholder="Enter Number">
            <button onclick="calculateCubeRoot()">Calculate</button>
            <div class="result" id="cube-root-result"></div>
        </div>
    </div>

    <script src="script.js"></script>
</body>
</html>
